{
  "name": "@standardbeagle/web-console",
  "version": "1.0.0",
  "description": "High-performance terminal component with binary WebSocket protocol for Go + React applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "terminal"
  ],
  "scripts": {
    "build": "tsc && rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build",
    "example:server": "go run examples/standalone-server/main.go",
    "example:wails": "cd examples/wails-app && wails dev"
  },
  "dependencies": {
    "@xterm/xterm": "^5.3.0",
    "@xterm/addon-fit": "^0.8.0",
    "@xterm/addon-web-links": "^0.9.0",
    "@xterm/addon-search": "^0.13.0",
    "@xterm/addon-canvas": "^0.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "keywords": [
    "terminal",
    "xterm",
    "websocket",
    "react",
    "go",
    "wails",
    "binary-protocol"
  ],
  "license": "MIT"
}